 $primary-color;\n    transition: 200ms;\n    cursor: pointer;\n    a {\n      color: black;\n    }\n    #title {\n      color: $primary-color;\n      font-size: 20px;\n    }\n    &:hover {\n      transform: scale(0.95);\n    }\n  }\n}\n"},"lineCount":null}},"error":null,"hash":"01067fc6ed9253bc3d0a5014b88cdaf7","cacheData":{"env":{}}}